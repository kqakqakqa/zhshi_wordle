{
  "app": {
    "bundleName": "com.github.kqakqakqa.zhshi.wordle",
    "vendor": "kqakqakqa",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "distroFilter": {
      "screenShape": {
        "policy": "include",
        "value": ["circle"]
      },
      "screenWindow": {
        "policy": "include",
        "value": ["454*454"]
      }
    },
    "abilities": [
      {
        "name": "default",
        "icon": "$media:icon",
        "label": "$string:zhshi_wordle",
        "type": "page"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/init_words/init_words",
          "pages/main/main"
        ],
        "name": "default"
      }
    ]
  }
}